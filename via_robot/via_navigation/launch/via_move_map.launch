<?xml version="1.0"?>
<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base"  output="screen">
    
    <rosparam file="$(find via_navigation)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find via_navigation)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />

    <rosparam file="$(find via_navigation)/params/map_nav/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find via_navigation)/params/map_nav/global_costmap_params.yaml" command="load" />

    <rosparam file="$(find via_navigation)/params/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find via_navigation)/params/move_base_params.yaml" command="load" />

    <param name="base_global_planner" type="string" value="navfn/NavfnROS"/>    
    <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>

		<remap from="odom" to="/odometry/filtered" />
		<remap from="cmd_vel" to="/via_velocity_controller/cmd_vel" />
    <remap from="map" to="/map" />

  </node>

</launch>

