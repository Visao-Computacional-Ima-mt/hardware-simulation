        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://via_description/meshes/link_1.STL" />
      </geometry>
    </collision>
  </link>

  <joint name="wheel_FR_joint" type="continuous">
    <origin
      xyz="0.49764 -0.87164 -0.44629"
      rpy="0 0 -1.5708" />
    <parent
      link="base_link" />
    <child
      link="wheel_FR" />
    <axis
      xyz="-1 0 0" />
    <limit
      effort="100"
      velocity="10" />
  </joint>
  
  <!-- rear right (RR) wheel -->
