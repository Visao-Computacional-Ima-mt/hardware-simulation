<?xml version="1.0" encoding="utf-8"?>
<robot name="via" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Include gabezo tags for simulation -->
  <xacro:include filename="$(find via_description)/urdf/via.gazebo.xacro"/>
  
  <!-- Dummy base link, because KDL cannot have inertia on the base link -->
  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0.752" rpy="0 0 0"/>
  </joint>

  <!-- base_link -->
  <link name="base_link">
    <inertial>
      <origin
        xyz="0.00038001 0.00058196 0.092591"
        rpy="0 0 0" />
      <mass
        value="114.07" />
      <inertia
        ixx="5.9365"
        ixy="5.8437E-06"
        ixz="0.0043726"
        iyy="2.3441"
        iyz="-4.9659E-05"
        izz="5.7375" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://via_description/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.18431 0.35686 1 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://via_description/meshes/base_link.STL" />
      </geometry>
    </collision>
  </link>


  <!-- front right (FR) wheel -->
  <link name="wheel_FR">
