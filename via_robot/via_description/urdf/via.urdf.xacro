        rpy="0 0 0" />
      <mass
        value="5.9259" />
      <inertia
        ixx="0.24268"
        ixy="-4.2738E-07"
        ixz="-1.3567E-07"
        iyy="0.12284"
        iyz="8.7511E-05"
